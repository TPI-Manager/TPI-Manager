{
  "version": 2,
  "rewrites": [
    {
      "source": "/api/(.*)",
      "destination": "/api/index.js"
    },
    {
      "source": "/socket.io/(.*)",
      "destination": "/api/index.js"
    },
    {
      "source": "/((?!api/|socket.io/|.*\\..*).*)",
      "destination": "/index.html"
    }
  ]
}
